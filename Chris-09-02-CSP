<script language="cache" method="cPopView" arguments="PSV,d1,d2,d3,d4,d5:%String">
	i PSV="" s SV="DSC"	
	i PSV=1 s SV="ASC"
	i PSV=2 s SV="DSC"
	i PSV=3 s SV="ASCPN"
	i PSV=4 s SV="DSCPN"
	i PSV=5 s SV="ASCAN"
	i PSV=6 s SV="DSCAN"
	i PSV=7 s SV="ASCIN"
	i PSV=8 s SV="DSCIN"
	d PopGetData^cAllAlerts
</script>

<script language="cache" method="cSort" arguments="d1,d2:%String">
	i d1=1 s SV="ASC"
	i d1=2 s SV="DSC"
