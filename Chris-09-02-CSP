function jPopView(btnval){
		jCleartr4A();
		sortby=btnval;
		#server(..cPopView(tempsort,btnval,T1.value,T2.value,R1.value,Drp1.value))#
}
function jCleartr4A(){
	rows=document.getElementById("tr4A").rows
	RowNo=rows.length
	if (RowNo>1){
		while (RowNo>1)
		{
			document.getElementById("tr4A").deleteRow(RowNo-1)
			RowNo=RowNo-1
		}
	}
}

function jSort(temp)
{	
	tempsort=temp;
	jPopView(sortby);
	//jCleartr4A();
	//#server(..cSort(temp))#
}

